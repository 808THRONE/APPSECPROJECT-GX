# Environment Variables Template
# Copy to .env and fill in your values

# PostgreSQL
POSTGRES_PASSWORD=iam_pass

# Redis
REDIS_PASSWORD=your_redis_password_here

# MinIO
MINIO_ROOT_USER=minioadmin
